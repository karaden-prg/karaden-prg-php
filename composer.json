{
    "name": "karaden-prg/karaden-prg-php",
    "type": "library",
    "description": "PHP library for the Karaden API.",
    "keywords": [
        "karaden",
        "sms",
        "communication platform as a service",
        "cpaas",
        "api"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Karaden\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karaden\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^2.3",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.14",
        "php-http/client-common": "^2.6",
        "psr/log": "^1.0 || ^2 || ^3",
        "php-http/logger-plugin": "^1.3"
    },
    "require-dev": {
        "php-http/mock-client": "^1.5",
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/psr7": "^2.4",
        "php-http/message": "^1.13"
    }
}
